{
    "name": "phuxtil/splfileinfo",
    "description": "SplFileInfo compatible implementation that allows to work with virtual (non existing) files",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8"
    },
    "require-dev": {
        "symfony/var-dumper": "^4|^5",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Phuxtil\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhuxtilTests\\Functional\\": "tests/Functional"
        }
    },
    "scripts": {
        "tests": [
            "vendor/bin/phpunit --testdox --no-coverage"
        ]
    }
}
